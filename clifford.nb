  RowBox[{"clifford", "[", 
   RowBox[{"s_", ",", "t_"}], "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"(", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"tmp", "=", 
     RowBox[{"cl", "[", 
      RowBox[{"s", ",", "t"}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"pseudoscalar", "=", 
     RowBox[{"tmp", "[", 
      RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"pseudoscalar", "=", 
       RowBox[{"pseudoscalar", ".", 
        RowBox[{"tmp", "[", 
