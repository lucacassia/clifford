  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"Cl", "(", 
      RowBox[{"0", ",", "1"}], ")"}], "=", 
     RowBox[{"<", "1"}]}], ",", 
    RowBox[{
     RowBox[{
      SubscriptBox["\[Sigma]", "2"], ">"}], " ", "=", 
     RowBox[{
      RowBox[{"\:211d", "(", "1", ")"}], "\[CircleTimes]", 
      RowBox[{"\:211d", "(", "1", ")"}]}]}]}], 
